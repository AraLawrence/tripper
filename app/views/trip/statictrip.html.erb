<h1>Your Trips</h1>

<% @trip.each do |trip| %>
<div class="trips">
<%= trip.trip_name%>
<div><%= link_to "Edit this Trip", trip_change_path(trip.id), class: "waves-effect waves-light btn black-text"%></div>
<div><%= link_to 'Delete Trip', method: :delete, class: "waves-effect waves-light btn black-text" %></div>
</div>
<% end %>